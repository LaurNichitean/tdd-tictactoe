{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "tic tac toe game in react",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --watch",
    "test": "npm run test:unit && npm run test:automation",
    "test:automation": "webpack && phantomjs dist/bundle.js",
    "test:unit": "node_modules/.bin/mocha --timeout 15000 --compilers js:babel-core/register  --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sheigel/tdd-tictactoe.git"
  },
  "keywords": [
    "live",
    "edit",
    "webpack"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "homepage": "https://github.com/sheigel/tdd-tictactoe",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.4.1",
    "chai-things": "^0.2.0",
    "chromedriver": "^2.20.0",
    "css-loader": "^0.23.1",
    "eslint": "1.10.3",
    "eslint-loader": "1.1.1",
    "exports-loader": "^0.6.2",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "mocha": "^2.3.4",
    "mocha-loader": "^0.7.1",
    "mocha-phantomjs": "^4.0.2",
    "phantomjs": "^1.9.19",
    "selenium-webdriver": "^2.48.2",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "body-parser": "1.14.2",
    "bootstrap": "^3.3.6",
    "es6-promise": "3.0.2",
    "express": "4.13.3",
    "isomorphic-fetch": "^2.2.0",
    "knockout": "3.4.0"
  }
}
